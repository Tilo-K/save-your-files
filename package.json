{
  "name": "save-your-files",
  "displayName": "Save your files!",
  "description": "A extension which saves your open files if you don't do it.",
  "version": "0.0.3",
  "publisher": "TiloKlarenbeek",
  "icon": "logo.png",
  "homepage": "https://klarenbeek.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tilo-K/save-your-files.git"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Education",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}
